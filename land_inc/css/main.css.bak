}
.footer-section {
    background-color: #f18b8b;
}
section.footer-section:before {
    content: "";
    background-image: url(../images/footer-line.png);
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
    position: relative;
    display: block;
    top: -74px;
    margin: auto;
    max-width: 1300px;
    width: 100%;
    height: 83px;
}
footer {
    padding-bottom: 70px;
}
footer p {
    color: #fff;
}
footer .title-page {
    font-weight: 600;
    font-size: 36px;

}
.about-us a {
    font-size: 18px;
    height: 44px;
    line-height: 44px;
    margin-top: 20px;
}
.rocket-2 {
    position: absolute;
    top: 770px;
    right: 0;
}
.line {
    height: 38px;
}
.line:after {
    content: '';
    width: 130%;
    height: 331px;
    display: block;

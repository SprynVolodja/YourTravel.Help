@font-face {
    font-family: 'Proxinova';
    src: url('fonts/proxima_nova_bold-webfont.eot');
    src: url('fonts/proxima_nova_bold-webfont.eot?#iefix') format('embedded-opentype'), url('fonts/proxima_nova_bold-webfont.woff2') format('woff2'), url('fonts/proxima_nova_bold-webfont.woff') format('woff'), url('fonts/proxima_nova_bold-webfont.ttf') format('truetype');
    font-weight: 700;
    font-style: normal;
    font-family: 'Proxinova';
    src: url('fonts/proxima_nova_semibold-webfont.eot');
    src: url('fonts/proxima_nova_semibold-webfont.eot?#iefix') format('embedded-opentype'), url('fonts/proxima_nova_semibold-webfont.woff2') format('woff2'), url('fonts/proxima_nova_semibold-webfont.woff') format('woff'), url('fonts/proxima_nova_semibold-webfont.ttf') format('truetype');
    font-weight: 600;
    font-style: normal;
    font-family: 'Proxinova';
    src: url('fonts/proxima_nova_regular-webfont.eot');
    src: url('fonts/proxima_nova_regular-webfont.eot?#iefix') format('embedded-opentype'), url('fonts/proxima_nova_regular-webfont.woff2') format('woff2'), url('fonts/proxima_nova_regular-webfont.woff') format('woff'), url('fonts/proxima_nova_regular-webfont.ttf') format('truetype');
    font-weight: 400;
    font-style: normal;
}
:root {
    --blue-color: #00d0da;
    --red-color: #e6a1a1;
}
.color-pink {
    color: var(--red-color) !important;
}
*,
*::after,
*::before {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.clear:after {
    clear: both;
    content: "";
    display: table;
}
ul,
li {
    list-style-type: none;
}
img:focus,
a:focus {
    outline: 0px
}
h1,
h2,
h3,
h4,
p {
    margin: 0;
}
.wrapper {
    max-width: 1300px;
    margin: auto;
    overflow: hidden;
}
.header-section {
    background: -webkit-linear-gradient(left, #ffb09c, #f9a7a5);
    /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(right, #ffb09c, #f9a7a5);
    /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(right, #ffb09c, #f9a7a5);
    /* For Firefox 3.6 to 15 */
    background: linear-gradient(to right, #ffb09c, #f9a7a5);
    /* Standard syntax */
    /*border-bottom: 10px solid #faa8a4;*/
}
header {
    padding: 24px 30px;
}
header .logo {
    float: left;
}
header .logo img {
    max-width: 100%;
}
header .header-h-cont {
    float: right;
    display: table;
    max-width: 770px;
    width: 75%;
}
header .header-h-cont > div {
    display: table-cell;
    vertical-align: middle;
}
header .login {}
header .login form {}
header .login form input {
    max-width: 150px;
    border-radius: 40px;
    outline: none;
    box-shadow: none;
    border: 2px solid #e6a1a1;
    padding: 4px 20px;
    font-size: 16px;
    margin-right: 15px;
}
header .login form input::-webkit-input-placeholder {
    color: #bbbbbb;
}
header .login form input::-moz-placeholder {
    color: #bbbbbb;
}
header .login form input:-ms-input-placeholder {
    color: #bbbbbb;
}
header .login form input:-moz-placeholder {
    color: #bbbbbb;
}
header .login form input[type=submit] {
    background-color: #00d0da;
    max-width: 108px;
    height: auto;
    border-color: transparent;
    outline: none;
    margin-right: 0;
}
header .login form input[type=submit]:active,
header .login form input[type=submit]:hover,
header .login form input[type=submit]:focus,
.btn,
.btn:active,
.btn:focus,
.btn:hover,
.btn:active:focus {
    outline: none;
}
header .register-btn {}
header .register-btn a {
    margin-right: 15px;
}
header .header_language {
    position: relative;
    display: inline-block;
    margin-left: 20px;
    font-size: 12px;
    color: #ffffff;
    width: 55px;
}
header .header_language_current {
    padding-right: 12px;
    min-height: 28px;
    cursor: pointer;
    position: relative;
}
header .header_language span {
    position: relative;
    top: 5px;
    font-size: 16px;
    width: 30px;
    display: block;
}
header .dropdown-icon {
    width: 26px;
    height: 28px;
    display: inline-block;
    margin-bottom: 0;
    vertical-align: middle;
    position: relative;
    /*background: rgba(255,255,255,.1);*/
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}
header .dropdown-icon i {
    font-size: 14px;
    color: #fff;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    position: absolute;
    width: 15px;
    height: 12px;
}
header .header_language_list {
    opacity: 0;
    visibility: hidden;
    position: absolute;
    z-index: 1111111;
    top: 50px;
    right: 0;
    width: 100%;
    background: #ffffff;
    color: #00d0da;
    border: 1px solid rgba(0, 0, 0, 0.05);
    border-radius: 4px;
    -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
    padding: 3px 0;
    transition: all 0.2s;
}
header .header_language_list li {
    padding: 2px 10px 2px 5px;
    cursor: pointer;
    text-align: center;
    transition: .2s;
}
.header_language_list li.current {
	background-color: rgba(239, 244, 246, 0.39);
}
header .header_language_list li:hover {
    color: #2b2f33;
    background: #eff4f6;
}
header .header_language_list.active {
    opacity: 1;
    visibility: visible;
    top: 35px;
}
.header_language_list::after {
    bottom: 100%;
    right: 6px;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(255, 255, 255, 0);
    border-bottom-color: #fff;
    border-width: 7px;
    margin-left: -7px;
}
.wrapper-first {
    background-image: url(../images/headerbg2.png);
    background-repeat: no-repeat;
    background-position: bottom center;
    padding-bottom: 60px;
    margin-bottom: -20px;
}
.wrapper-second {
    position: relative;
    /*background: #faa8a4;
    background: linear-gradient(to right, #ffaf9a, #f9a7a7);*/
    padding-top: 70px;
}
.play-video {
    display: block;
    width: 129px;
    height: 129px;
    margin: 80px auto 68px;
}
.title-page {
    font-family: 'Proxinova';
    font-weight: 600;
    color: #fff;
    font-size: 36px;
}
.our-service-text {
    color: #fff;
    font-size: 24px;
    margin: 45px 0 30px;
    font-family: 'Proxinova';
    font-weight: 400;
}
.btn {
    width: 188px;
    height: 33px;
    color: #fff;
    padding: 0;
    font-family: 'Proxinova';
    font-weight: 600;
    border-radius: 19px;
    font-size: 16px;
}
.btn-big {
    width: 237px;
    height: 55px;
    line-height: 55px;
    font-size: 21px;
    border-radius: 40px;
}
.btn-register {
    width: auto;
    padding: 0 28px;
    border: 2px solid #fff;
    height: 34px;
    line-height: 30px;
    font-size: 16px;
    border-radius: 40px;
}
.btn:focus,
.btn:active,
.btn:hover {
    color: #fff;
}
.btn-blue {
    background-color: var(--blue-color);
}
.btn-red {
    background-color: var(--red-color);
}
.btn-starts {
    text-align: center;
    margin-bottom: 61px;
}
.btn-starts .btn {
    width: 234px;
}
.btn-starts .btn + .btn {
    margin-left: 31px;
}
.scroll-down {
    display: inline-block;
    height: 91px;
    width: 91px;
    border: 3px solid #fff;
    border-radius: 50%;
    margin: auto;
    background-color: #f18b8b;
}
.scroll-down img {
    padding-top: 18px;
}
.rocket-1 {
    position: absolute;
    top: 15%;
    right: 15px;
}
.join-us {
    /*background: #faa8a4;*/
    background: -webkit-linear-gradient(left, #ffb09c, #f9a7a5);
    background: -o-linear-gradient(right, #ffb09c, #f9a7a5);
    background: -moz-linear-gradient(right, #ffb09c, #f9a7a5);
    background: linear-gradient(to right, #ffb09c, #f9a7a5);
}
.join-us .title-page {
    font-size: 30px;
    text-transform: uppercase;
}
.circles {
    position: relative;
    width: 825px;
    height: 825px;
    margin-top: -34px;
}
.circle-3,
.circle-2,
.circle-1 {
    position: absolute;
    border-radius: 50%;
}
.circle-1 {
    top: 212px;
    left: 143px;
    background-color: rgba(255, 255, 255, 0.349);
    width: 401px;
    height: 401px;
}
.circle-2 {
    top: 83px;
    left: 17px;
    background-color: rgba(255, 255, 255, 0.251);
    width: 659px;
    height: 659px;
}
.circle-3 {
    background-color: rgba(255, 255, 255, 0.102);
    left: -66px;
    width: 825px;
    height: 825px;
}
.man-on-rocket {
    position: absolute;
    top: 174px;
    left: -24px;
}
.panel {
    width: 486px;
    border-radius: 3px;
    background-color: rgb(241, 139, 139);
    box-shadow: 0px 4px 19.53px 1.47px rgba(37, 106, 142, 0.33);
    margin-left: 19px;
    margin-bottom: 0;

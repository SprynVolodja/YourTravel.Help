}
#show-video .modal-dialog .modal-body {position: relative; padding-bottom: 56.25%; padding-top: 25px; height: 0;}
#show-video .modal-dialog .modal-body iframe {position: absolute; top: 0; left: 0; right: 0; width: 100%; height: 100%; margin: auto;}
#show-video .modal-dialog .modal-content {background: none; border: none;}
@media screen and (min-width: 768px) {
	#show-video .modal-dialog {width: 70%;}
}
@media (max-width: 1200px) {
    .rocket-1 {

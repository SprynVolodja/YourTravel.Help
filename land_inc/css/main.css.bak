@media (max-width: 998px) {
    .rocket-1 {
        width: 150px;
        top: 113px
    }
    h1.title-page {
        font-size: 33px;
    }
    .phone {
        text-align: center;
    }
    .phone p,
    .phone img {
        padding-right: 0;
    }
    .map {
        text-align: center;
    }
    .map img {
        padding-left: 0;
    }
}

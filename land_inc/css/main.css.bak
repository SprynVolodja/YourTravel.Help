.panel .title {
    color: #fff;
}
.registration {
    margin-top: 128px;
    padding: 38px 63px 31px;
    height: 515px;
}
.registration .title {
    font-weight: 700;
    font-size: 26px;
}
.registration form {
    margin-top: 40px;
}
.registration label {
    font-weight: 400;
    color: #fff;
